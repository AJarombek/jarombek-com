{
  "name": "jarombek-com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client:server:test": "jest --coverage --updateSnapshot",
    "client:stats": "webpack --env clientProduction --profile --json > client-stats.json",
    "client:build": "webpack --env clientProduction",
    "client:dev": "webpack --env clientDevelopment",
    "client:start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env clientDevelopment --open\"",
    "server:stats": "webpack --env serverProduction --profile --json > server-stats.json",
    "server:build": "webpack --env serverProduction",
    "server:dev": "webpack --env serverDevelopment",
    "server:start": "webpack --env serverDevelopment --watch --display-error-details",
    "server:deploy": "node ./dist/server/bundle.js"
  },
  "repository": "git+https://github.com/AJarombek/jarombek-com.git",
  "keywords": [],
  "author": "Andrew Jarombek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AJarombek/jarombek-com/issues"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setupTests.js",
    "moduleNameMapper": {
      "\\.(scss)$": "jest-css-modules",
      "\\.(png|ttf)$": "../../mocks/fileMock.js"
    },
    "collectCoverageFrom": [
      "src/**"
    ],
    "coveragePathIgnorePatterns": [
      "./src/client/index.js",
      "./src/client/status/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "homepage": "https://github.com/AJarombek/jarombek-com#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "base64-url": "^2.2.0",
    "bcrypt-nodejs": "^0.0.3",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "highlightjs": "^9.10.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-style-loader": "^4.0.0",
    "moment": "^2.22.0",
    "mongoose": "^5.0.15",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "uuid": "^3.2.1",
    "webpack": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "^6.3.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.2",
    "nodemon-webpack-plugin": "^0.1.6",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "sinon": "^6.0.0",
    "style-loader": "^0.20.3",
    "supertest": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-dev": "^0.3.0"
  }
}
