  "scripts": {
    "client:test": "jest --config jest.client-config.js",
    "client:test:update": "jest --updateSnapshot --config jest.client-config.js",
    "client:cy:open": "cypress open",
    "client:cy:run": "cypress run",
    "client:cy:run-headless": "cypress run --headless",
    "server:test": "export port=30001 && jest --config jest.server-config.js",
    "server:test:update": "export port=30001 && jest --updateSnapshot --config jest.server-config.js",
    "client:stats": "webpack --env clientProduction --profile --json > client-stats.json",
    "client:build": "webpack --env clientProduction",
    "client:dev": "webpack --env clientDevelopment",
    "client:start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env clientDevelopment --open\"",
    "server:stats": "webpack --env serverProduction --profile --json > server-stats.json",
    "server:build": "webpack --env serverProduction",
    "server:dev": "webpack --env serverDevelopment",
    "server:start": "webpack --env serverDevelopment --watch --display-error-details",
    "server:deploy": "node ./dist/server/bundle.js"
  },
