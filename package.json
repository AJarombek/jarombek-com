{
  "name": "jarombek-com",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "client:test": "jest --config jest.client-config.js",
    "client:test:update": "jest --updateSnapshot --config jest.client-config.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run-headless": "cypress run --headless",
    "server:test": "export port=30001 && jest --config jest.server-config.js",
    "server:test:update": "export port=30001 && jest --updateSnapshot --config jest.server-config.js",
    "client:stats": "webpack --env webpack_env=clientProduction --profile --json > client-stats.json",
    "client:build": "webpack --env webpack_env=clientProduction",
    "client:dev": "webpack --env webpack_env=clientDevelopment",
    "client:start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env webpack_env=clientDevelopment --open\"",
    "server:stats": "webpack --env webpack_env=serverProduction --profile --json > server-stats.json",
    "server:build": "webpack --env webpack_env=serverProduction",
    "server:dev": "webpack --env webpack_env=serverDevelopment",
    "server:start": "webpack --env webpack_env=serverDevelopment --watch --display-error-details",
    "server:deploy": "node ./dist/server/bundle.js",
    "start:local": "export NODE_ENV=local && yarn client:dev && yarn server:dev && yarn server:deploy",
    "lint": "eslint src",
    "prettier:check": "prettier src --check",
    "prettier:write": "prettier src --write"
  },
  "repository": "git+https://github.com/AJarombek/jarombek-com.git",
